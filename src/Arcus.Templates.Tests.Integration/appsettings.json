{
  "Arcus": {
    "SubscriptionId": "#{Arcus.Templates.SubscriptionId}#",
    "TenantId": "#{Arcus.Templates.TenantId}#",
    "ResourceGroupName": "#{Arcus.Templates.ResourceGroupName}#",
    "ServicePrincipal": {
      "ClientId": "#{Arcus.Templates.ServicePrincipal.ClientId}#",
      "ClientSecret": "#{Arcus.Templates.ServicePrincipal.ClientSecret}#"
    },
    "ServiceBus": {
      "Namespace": "#{Arcus.Templates.ServiceBus.Namespace}#"
    },
    "EventHubs": {
      "Namespace": "#{Arcus.Templates.EventHubs.Namespace}#"
    },
    "StorageAccount": {
      "Name": "#{Arcus.Templates.StorageAccount.Name}#"
    },
    "ApplicationInsights": {
      "ConnectionString": "#{Arcus.Templates.ApplicationInsights.ConnectionString}#",
      "LogAnalytics": {
        "WorkspaceId": "#{Arcus.Templates.ApplicationInsights.LogAnalytics.WorkspaceId}#"
      }
    },
    "Api": {
      "BaseUrl": "#{Arcus.Api.BaseUrl}#"
    },
    "AzureFunctions": {
      "AzureWebJobsStorage": "#{Arcus.AzureFunctions.AzureWebJobsStorage}#",
      "Http": {
        "Isolated": {
          "HttpPort": "#{Arcus.AzureFunctions.Http.Isolated.HttpPort}#"
        },
        "InProcess": {
          "HttpPort": "#{Arcus.AzureFunctions.Http.InProcess.HttpPort}#"
        }
      }
    }
  },
  "Build.SourcesDirectory": "#{Build.SourcesDirectory}#"
}